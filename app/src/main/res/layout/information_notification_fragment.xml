<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="774px"
    android:layout_height="354px"
    tools:context=".ui.notification.information.InformationNotificationFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout

            android:layout_width="472px"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.5">

            <LinearLayout
                android:layout_width="467px"
                android:layout_height="60px"
                android:layout_marginTop="1dp"
                android:layout_marginBottom="1dp"
                android:orientation="horizontal">

                <TextView
                    android:textAllCaps="true"
                    android:id="@+id/NumberNotification"
                    android:layout_width="317px"
                    android:layout_height="60px"
                    android:background="@drawable/ic_boxnumberktk"
                    android:fontFamily="@font/source_sans_pro_semibold"
                    android:gravity="center"
                    android:paddingStart="10px"
                    android:text="AAAA 1234567"
                    android:textAlignment="textStart"
                    android:textColor="#183961"
                    android:textSize="40px" />

                <TextView
                    android:textAllCaps="true"
                    android:id="@+id/isoNumberNotification"
                    android:layout_width="149px"
                    android:layout_height="60px"
                    android:background="@drawable/ic_boxnumberktk"
                    android:fontFamily="@font/source_sans_pro_semibold"
                    android:gravity="center"
                    android:paddingStart="10px"
                    android:text="AA AA"
                    android:textAlignment="center"
                    android:textColor="#183961"
                    android:textSize="40px" />
            </LinearLayout>

            <TextView
                android:id="@+id/targetManifest"
                android:layout_width="467px"
                android:layout_height="60px"
                android:layout_marginBottom="1dp"
                android:background="#183961"
                android:fontFamily="@font/source_sans_pro_semibold"
                android:gravity="center"
                android:paddingStart="10px"
                android:text="Contship Ace 049 050"
                android:textAlignment="textStart"
                android:textColor="@color/colorWhite"
                android:textSize="40px" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="467px"
                android:layout_height="60px"
                android:layout_marginBottom="1dp"
                android:background="#183961"
                android:fontFamily="@font/source_sans_pro_semibold"
                android:gravity="center"
                android:paddingStart="10px"
                android:text="На авто"
                android:textAlignment="textStart"
                android:textColor="@color/colorWhite"
                android:textSize="40px"
                android:visibility="invisible" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="1dp"
                android:orientation="horizontal"
                android:visibility="invisible">

                <TextView

                    android:id="@+id/textView9"
                    android:layout_width="233px"
                    android:layout_height="60px"
                    android:layout_marginRight="1px"
                    android:background="#83DE91"
                    android:fontFamily="@font/source_sans_pro_semibold"
                    android:gravity="center"
                    android:paddingStart="10px"
                    android:text="Импорт"
                    android:textAlignment="textStart"
                    android:textColor="#183961"
                    android:textSize="40px" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="233px"
                    android:layout_height="60px"
                    android:background="#83DE91"
                    android:fontFamily="@font/source_sans_pro_semibold"
                    android:gravity="center"
                    android:paddingStart="10px"
                    android:text="Груженый"
                    android:textAlignment="textStart"
                    android:textColor="#183961"
                    android:textSize="40px" />
            </LinearLayout>

            <CheckBox
                android:id="@+id/checkBox"
                android:layout_width="match_parent"
                android:layout_height="60px"
                android:fontFamily="@font/source_sans_pro_semibold"
                android:text="Нет штатного места"
                android:textColor="#183961"
                android:textSize="32px"
                android:textStyle="bold" />

        </LinearLayout>

        <TextView
            android:id="@+id/textView14"
            android:layout_width="132px"
            android:layout_height="35px"
            android:fontFamily="@font/source_sans_pro"
            android:text="ИСО - код"
            android:textAlignment="center"
            android:textColor="#183961"
            android:textSize="26px"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.85"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.065" />

        <EditText
            android:inputType="textCapCharacters"
            android:contentDescription="hf_use_description|Исо-код"
            android:id="@+id/editText4"
            android:layout_width="132px"
            android:layout_height="50px"
            android:background="@drawable/ic_rectangle"
            android:ems="10"
            android:text="AA AA"
            android:textAlignment="center"
            android:textColor="#183961"
            android:textSize="30px"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.85"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.181" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="132px"
            android:layout_height="35px"
            android:fontFamily="@font/source_sans_pro_semibold"
            android:text="Тара"
            android:textAlignment="center"
            android:textColor="#183961"
            android:textSize="26px"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.85"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.385" />

        <EditText
            android:contentDescription="hf_use_description|Тара"
            android:id="@+id/editText3"
            android:layout_width="132px"
            android:layout_height="50px"
            android:background="@drawable/ic_rectangle"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="19245"
            android:textAlignment="center"
            android:textColor="#183961"
            android:textSize="30px"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.85"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.517" />

        <Button
            android:id="@+id/button3"
            android:layout_width="132px"
            android:layout_height="70px"
            android:text="Далее"
            android:textSize="15px"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.85"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.731" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>